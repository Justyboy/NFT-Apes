    <script>
      $('a').click(function(e){
        var current_path = window.location.pathname;
        var href = $(this).attr('href');
        
        if(href.includes(current_path) && href.includes('#')){
          e.preventDefault();
          var anchor = href.substring(href.indexOf("#")+1);
          console.log('anchor: ', anchor);
          window.location.hash = anchor;
        }else{
        }
        
        console.log('href');
        console.log(current_path);
      });
      
      $('.faq-category-heading').click(function(e){
        var $parent_el = $(this).parents('.faq-category-container');
        var $content = $parent_el.find('.faq-list');
        if($parent_el.hasClass('open')){
          $content.slideUp(400);
          $parent_el.removeClass('open');
        }else{
          $content.slideDown(400);
          $parent_el.addClass('open');
        }
      });
      
      $('.qty-control').click(function(e){
        var qty_input = $('#Quantity');
        var current_val = parseInt(qty_input.val());
        var new_val = 1;
        
        if($(this).hasClass('minus')){
          if(current_val - 1 > 0 ){
            new_val = current_val - 1;
          }else{
            new_val = 1;
          }
        }else{
          new_val = current_val + 1;
        }
        
        qty_input.val(new_val);
      });
      
      
      $(document).ready(function(){
      //reset height of tabbed collections 
        
        PaloAlto.debounce($(window).resize(function(){
          var $tabbedCol = $('.collection__tabs');
          if($tabbedCol.length > 0){
            $tabbedCol.css('height', 'initial');
            var height = $tabbedCol.innerHeight();
            $tabbedCol.css('height', height);
          }
        }), 300);
        
		setTimeout(function(){ 
          $(window).trigger('resize');
        }, 3000);
        
      });
    </script>
    
